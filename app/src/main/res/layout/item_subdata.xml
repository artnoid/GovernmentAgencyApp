<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="horizontal"
    app:cardBackgroundColor="#00644b"
    android:id="@+id/itemsubdata_cv"
    android:background="#00644b">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_margin="8dp"
        android:padding="10dp"
        android:background="#ffffff"
        android:orientation="vertical">

    <TextView
        android:id="@+id/tvTitle"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#ffffff"
        android:text="TITLE"
        android:textAlignment="center"
        android:textColor="#00644b"
        android:textSize="12pt"/>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="horizontal"
        android:background="#00644b"
        android:padding="5dp"
        android:layout_marginTop="20dp">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1.9"
            android:orientation="vertical">

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:padding="5dp"
                android:text="ID"
                android:textColor="#ffffff"
                android:textSize="8pt"/>

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:padding="5dp"
                android:text="ID District"
                android:textColor="#ffffff"
                android:textSize="8pt"/>

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:padding="5dp"
                android:text="District"
                android:textColor="#ffffff"
                android:textSize="8pt"/>

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:padding="5dp"
                android:text="Agency"
                android:textColor="#ffffff"
                android:textSize="8pt"/>

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:padding="5dp"
                android:text="Office"
                android:textColor="#ffffff"
                android:textSize="8pt"/>

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:padding="5dp"
                android:text="Telephone"
                android:textColor="#ffffff"
                android:textSize="8pt"/>

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:padding="5dp"
                android:text="Address"
                android:textColor="#ffffff"
                android:textSize="8pt"/>
        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:padding="5dp"
                android:text=":"
                android:textColor="#ffffff"
                android:textSize="8pt"/>

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:padding="5dp"
                android:text=":"
                android:textColor="#ffffff"
                android:textSize="8pt"/>

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:padding="5dp"
                android:text=":"
                android:textColor="#ffffff"
                android:textSize="8pt"/>

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:padding="5dp"
                android:text=":"
                android:textColor="#ffffff"
                android:textSize="8pt"/>

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:padding="5dp"
                android:text=":"
                android:textColor="#ffffff"
                android:textSize="8pt"/>

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:padding="5dp"
                android:text=":"
                android:textColor="#ffffff"
                android:textSize="8pt"/>

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:padding="5dp"
                android:text=":"
                android:textColor="#ffffff"
                android:textSize="8pt"/>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:orientation="vertical">

            <TextView
                android:id="@+id/tvId"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:padding="5dp"
                android:text="ID"
                android:textColor="#ffffff"
                android:textSize="8pt"/>

            <TextView
                android:id="@+id/tvIdKabupaten"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:padding="5dp"
                android:text="ID_KABUPATEN"
                android:textColor="#ffffff"
                android:textSize="8pt"/>

            <TextView
                android:id="@+id/tvNamaKabupaten"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:padding="5dp"
                android:text="NAMA_KABUPATEN"
                android:textColor="#ffffff"
                android:textSize="8pt"/>

            <TextView
                android:id="@+id/tvJenisInstansi"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:padding="5dp"
                android:text="JENIS_INSTANSI"
                android:textColor="#ffffff"
                android:textSize="8pt"/>

            <TextView
                android:id="@+id/tvNamaInstansi"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:padding="5dp"
                android:text="NAMA_INSTANSI"
                android:textColor="#ffffff"
                android:textSize="8pt"/>

            <TextView
                android:id="@+id/tvNomorInstansi"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:padding="5dp"
                android:text="NOMOR_INSTANSI"
                android:textColor="#ffffff"
                android:textSize="8pt"/>

            <TextView
                android:id="@+id/tvAlamatInstansi"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:padding="5dp"
                android:text="ALAMAT_INSTANSI"
                android:textColor="#ffffff"
                android:textSize="8pt"/>
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/tvLat"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:padding="5dp"
                    android:text="LAT"
                    android:textColor="#ffffff"
                    android:textSize="8pt"/>

                <TextView
                    android:id="@+id/tvLong"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:padding="5dp"
                    android:text="LONG"
                    android:textColor="#ffffff"
                    android:textSize="8pt"
                    android:layout_marginBottom="10dp"/>
            </LinearLayout>


        </LinearLayout>

    </LinearLayout>

    </LinearLayout>
</androidx.cardview.widget.CardView>